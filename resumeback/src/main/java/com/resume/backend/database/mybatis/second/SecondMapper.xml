<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.resume.backend.database.mybatis.second.SecondMapper">
	<select id="databaseTest"
		resultType="com.resume.backend.database.model.TestVo">
		SELECT
		id AS id,
		pw AS pw
		FROM logintest
		WHERE id = #{id}
	</select>

</mapper>
